{
  "name": "icosa-viewer",
  "version": "0.7.2",
  "description": "Viewer for Tilt Brush / Open Brush files and derivatives",
  "scripts": {
    "cp:threeicosa": "ncp node_modules/three-icosa/dist/three-icosa.module.js dist/lib/three-icosa.module.js",
    "cp:tiltloader": "ncp node_modules/three-tiltloader/dist/three-tiltloader.module.js dist/lib/three-tiltloader.module.js",
    "cp:cameracontrols": "ncp node_modules/camera-controls/dist/camera-controls.module.js dist/lib/camera-controls.module.js",
    "cp:holdevent": "ncp node_modules/hold-event/dist/hold-event.module.js dist/lib/hold-event.module.js",
    "copy": "npm run cp:threeicosa & npm run cp:tiltloader & npm run cp:cameracontrols & npm run cp:holdevent",
    "build": "tsc --build & npm run copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icosa-gallery/icosa-viewer.git"
  },
  "author": "Icosa Gallery",
  "license": "Apache-2.0",
  "main": "dist/Viewer.js",
  "module": "dist/icosa-viewer.module.js",
  "types": "dist/icosa-viewer.d.ts",
  "bugs": {
    "url": "https://github.com/icosa-gallery/icosa-viewer/issues"
  },
  "homepage": "https://github.com/icosa-gallery/icosa-viewer#readme",
  "devDependencies": {
    "@types/three": "^0.139.0",
    "typescript": "^4.1.3",
    "ncp": "^2.0.0"
  },
  "dependencies": {
    "camera-controls": "^1.34.2",
    "hold-event": "0.0.1",
    "three": "^0.139.2",
    "three-icosa": "^0.4.0",
    "three-tiltloader": "^0.4.1"
  },
  "files": [
    "dist",
    "examples"
  ],
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/icosa"
  },
  "keywords": [
    "three",
    "three.js",
    "viewer",
    "openbrush",
    "tiltbrush",
    "creative",
    "camera",
    "vr",
    "xr"
  ]
}
