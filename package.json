{
  "name": "icosa-viewer",
  "version": "0.7.3-alpha.2",
  "description": "Viewer for Tilt Brush / Open Brush files and derivatives",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build --no-cache",
    "build:local": "parcel build --no-cache && shx cp \"../three-icosa-main/dist/three-icosa.module.js\" \"dist/three-icosa.module.js\"",
    "download-three-icosa": "curl -o dist/three-icosa.module.js https://cdn.jsdelivr.net/npm/three-icosa@0.4.2-alpha.18/dist/three-icosa.module.js",
    "setup-cdn": "node scripts/setup-cdn-importmap.js",
    "test": "node npm-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icosa-foundation/icosa-viewer.git"
  },
  "author": "Icosa Gallery",
  "license": "Apache-2.0",
  "module": "dist/icosa-viewer.module.js",
  "default": "src/icosa-viewer.js",
  "source": "src/viewer.ts",
  "sourceMaps": true,
  "targets": {
    "module": {
      "outputFormat": "esmodule",
      "isLibrary": true,
      "includeNodeModules": {
        "three": false,
        "three-tiltloader": false,
        "three-icosa": false,
        "@sparkjsdev/spark": false
      }
    }
  },
  "bugs": {
    "url": "https://github.com/icosa-foundation/icosa-viewer/issues"
  },
  "homepage": "https://github.com/icosa-foundation/icosa-viewer#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@types/three": "^0.168",
    "parcel": "^2.12.0",
    "typescript": "^5.4.5",
    "shx": "^0.3.4"
  },
  "dependencies": {
    "@depasquale/three-immersive-controls": "^0.0.15",
    "camera-controls": "^2.8.4",
    "hold-event": "0.0.1",
    "three-icosa": "^0.4.2-alpha.18",
    "three-tiltloader": "^0.4.2-alpha.4"
  },
  "peerDependencies": {
    "three": ">=0.165 <0.166.0",
    "@sparkjsdev/spark": "^0.1.8"
  },
  "peerDependenciesMeta": {
    "@sparkjsdev/spark": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "examples"
  ],
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/icosa"
  },
  "keywords": [
    "three",
    "three.js",
    "viewer",
    "openbrush",
    "tiltbrush",
    "creative",
    "camera",
    "vr",
    "xr"
  ]
}
